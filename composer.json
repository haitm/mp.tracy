{
  "name": "haitm/mptracy",
  "description": "Slim Framework Debugger",
  "license": "Apache-2.0",
  "type": "library",
  "keywords": [
    "Slim Framework",
    "MpTracy",
    "Tracy",
    "Debug bar"
  ],
  "authors": [
    {
      "name": "Pandog"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "slim/slim": "~3.7",
    "tracy/tracy": "~2.4"
  },
  "autoload": {
    "psr-4": {
      "MpTracy\\": "src/MpTracy"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@phpunit"
    ],
    "phpunit": "php vendor/bin/phpunit"
  }
}
